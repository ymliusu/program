{
    "title": "Program",
    "author": "ymliusu",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles": {
        "website": "styles/website.css"
     },
    "plugins": [
      "-lunr", 
      "-search",
      "-sharing",
      "search-plus", 
      "emphasize",
      "back-to-top-button",
      "tbfed-pagefooter",
      "code",
      "splitter",
      "github",
      "-highlight",
      "prism",
      "prism-themes"
      ],
    "pluginsConfig": {
      "theme-default": {
        "showLevel": true
       },
      "tbfed-pagefooter": {
        "copyright":"Copyright &copy ymliusu 2021",
        "modify_label": "修订日期：",
        "modify_format": "YYYY-MM-DD"
       },
       "code": {
        "copyButtons": true
      },
      "github": {
        "url": "https://github.com/ymliusu"
      },
      "prism": {
        "css": [
            "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
        ]
      }
    }
}
